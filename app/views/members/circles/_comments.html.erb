<% @circle.cir_comments.each do |cir_comment| %>
  <%=cir_comment.member.name %>
  <%= cir_comment.created_at.strftime('%Y/%m/%d %H:%M') %>
  <%= cir_comment.comment %>
  <% if cir_comment.member == current_member %>
    <%= link_to "delete", circle_cir_comment_path(cir_comment.circle, cir_comment), method: :delete %>
  <% end %>


<% end %>