<h2>Sign up</h2>

<%= form_with model: @member, url: member_registration_path, local: true do |f| %>
  <%= render "members/shared/error_messages", resource: @aim %>

  <div class="aim_w_field">
    <%= f.label :aim_w %><br />
    <%= f.number_field :aim_w %>
  </div>

  <div class="aim_bf_field">
    <%= f.label :aim_bf %><br />
    <%= f.number_field :aim_bf %>
  </div>

   <div class="aim_mus_field">
    <%= f.label :aim_mus %><br />
    <%= f.number_field :aim_mus %>
  </div>

  <div class="aim_sm_field">
    <%= f.label :aim_sm %><br />
    <%= f.number_field :aim_sm %>
  </div>

<%= f.submit "registration", class: "btn-default btn-red", url: "aims_path", method: :post %>
<%= render "members/shared/links" %>
<%#= render "/registration/registration_footer" %>
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>


